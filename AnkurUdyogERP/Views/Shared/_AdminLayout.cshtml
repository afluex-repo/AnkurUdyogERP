                                <img class="b-r-50" src="../assets/images/dashboard/profile.png" alt="">
                                <div class="flex-grow-1">
                                    <span>@Session["LoginId"]</span>
                                    @*<p class="mb-0 font-roboto">Admin <i class="middle fa fa-angle-down"></i></p>*@
                                </div>
                            </div>
                            <ul class="profile-dropdown onhover-show-div">
                                <li><a href="#"><i data-feather="user"></i><span>Profile</span></a></li>
                                <li><a href="/Home/Login"><i data-feather="log-in"> </i><span>Log Out</span></a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
